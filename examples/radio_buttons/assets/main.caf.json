{

"#manifest": {
    "": "examples.radio_buttons"
},

"#import": {
    "builtin.widgets.radio_button": ""
},

"scene": {
    "FlexStyle": {
        "dims": {"width": {"Vw": 100.0}, "height": {"Vh": 100.0}},
        "content": {"flex_direction": "Column", "justify_main": "Center", "justify_cross": "Center"}
    },
    "BackgroundColor": [{"Hsla": {"hue": 30.0, "saturation": 0.09, "lightness": 0.72, "alpha": 1.0}}],

    "display": {
        "FlexStyle": {
            "dims": {"border": {"top": {"Px": 2.0}, "bottom": {"Px": 2.0}, "left": {"Px": 2.0}, "right": {"Px": 2.0}}},
            "content": {"justify_main": "Center", "justify_cross": "Center"}
        },
        "BackgroundColor": [{"Hsla": {"hue": 212.0, "saturation": 0.33, "lightness": 0.45, "alpha": 1.0}}],
        "BorderColor": [{"Hsla": {"hue": 174.0, "saturation": 0.23, "lightness": 0.18, "alpha": 1.0}}],
        "BrRadius": [{"Px": 6.0}],

        "text": {
            "FlexStyle": {
                "flex": {"margin": {"top": {"Px": 5.0}, "bottom": {"Px": 5.0}, "left": {"Px": 9.0}, "right": {"Px": 9.0}}}
            },
            "TextLine": {"size": 25.0}
        }
    },

    "radio_frame": {
        "FlexStyle": {
            "dims": {"border": {"top": {"Px": 2.0}, "bottom": {"Px": 2.0}, "left": {"Px": 2.0}, "right": {"Px": 2.0}}},
            "content": {
                "flex_direction": "Column",
                "justify_main": "SpaceEvenly",
                "justify_cross": "Center",
                "padding": {"top": {"Px": 8.0}, "bottom": {"Px": 8.0}, "left": {"Px": 10.0}, "right": {"Px": 10.0}}
            },
            "flex": {"margin": {"top": {"Px": 20.0}}}
        },
        "BackgroundColor": [{"Hsla": {"hue": 138.0, "saturation": 0.23, "lightness": 0.57, "alpha": 1.0}}],
        "BorderColor": [{"Hsla": {"hue": 174.0, "saturation": 0.23, "lightness": 0.18, "alpha": 1.0}}],
        "BrRadius": [{"Px": 6.0}]
    }
},

"radio_button(#spec:radio_button_default)": {
    "@text_margin": {"top": {"Px": 5.0}, "bottom": {"Px": 5.0}, "left": {"Px": 10.0}, "right": {"Px": 10.0}},
    "!base_flex": {"margin": {"top": {"Px": 5.0}, "bottom": {"Px": 5.0}, "left": {"Px": 5.0}, "right": {"Px": 5.0}}}
}

}
